<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Background.jpg" />
    <None Remove="Images\person1.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="Resources\BrushesDictionary.xaml" />
    <Page Remove="Resources\GeometryDrawingDictionary.xaml" />
    <Page Remove="Resources\StylesDictionary.xaml" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\HomeMangager.DAL\HomeMangager.DAL.csproj" />
    <ProjectReference Include="..\HomeMangager.Model\HomeMangager.Model.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Background.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\person1.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\BrushesDictionary.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Resource>
    <Resource Include="Resources\GeometryDrawingDictionary.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Resource>
    <Resource Include="Resources\StylesDictionary.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Messages\" />
    <Folder Include="Services\" />
    <Folder Include="Utilities\" />
    <Folder Include="ViewModel\Dagboek\" />
    <Folder Include="ViewModel\Budget\" />
    <Folder Include="ViewModel\Homepage\" />
    <Folder Include="ViewModel\StickyNotes\" />
    <Folder Include="ViewModel\Security\" />
    <Folder Include="ViewModel\Todo\" />
    <Folder Include="ViewModel\Personen\" />
    <Folder Include="View\Budget\" />
    <Folder Include="View\Homepage\" />
    <Folder Include="View\StickyNotes\" />
    <Folder Include="View\Security\" />
    <Folder Include="View\Todo\" />
  </ItemGroup>

</Project>
